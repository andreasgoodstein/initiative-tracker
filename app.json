{
  "expo": {
    "name": "The Initiative Tracker",
    "slug": "the-initiative-tracker",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0EA5FF"
    },
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/andreasgoodstein/initiative-tracker",
    "description": "A simple Typescript app, meant to keep track of initiative order in games such as D&D 5e"
  }
}
