{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start-web": "parcel App.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:update": "node node_modules/jest/bin/jest.js --silent -u",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "test": "node node_modules/jest/bin/jest.js --silent --bail --noStackTrace",
    "build": "tsc --noEmit",
    "bundle": "parcel build App.js",
    "lint": "tslint -p tsconfig.json",
    "publish": "expo publish",
    "pre-push": "npm run build && npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push"
    }
  },
  "dependencies": {
    "expo": "^34.0.1",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/expo": "^32.0.13",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-redux": "^7.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "expo-cli": "3.0.10",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-expo": "^34.0.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-react-native-web": "^0.6.1",
    "react-dom": "16.8.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2"
  }
}
